function rollOver(img, name)
{
if (img != selected) {
 img.src = "images/"+name+"_over.png";
 }
}

function rollOut(img, name)
{
if (img != selected) {
 img.src = "images/"+name+"_out.png";
 }
}

function setSelected(img, name)
{
	if (selected) selected.src = "images/"+selectedName+"_out.png";
	selected = img;
	selectedName = name;
	selected.src = "images/"+selectedName+"_selected.png";
	document.getElementById("frame").height = "340";
	document.getElementById("frame").src = "Classes_"+name+".html";
}
var selected;
var selectedName
